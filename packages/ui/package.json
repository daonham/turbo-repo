{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./src/index.tsx",
      "types": "./src/index.tsx",
      "require": "./src/index.tsx"
    },
    "./icons/*": {
      "import": "./src/icons/*.tsx",
      "types": "./src/icons/*.tsx",
      "require": "./src/icons/*.tsx"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint src/ --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/utils": "workspace:*",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "4.1.16",
    "tsup": "^8.5.0",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@tailwindcss/postcss": "4.1.16",
    "@tanstack/react-table": "^8.21.3",
    "@tiptap/extension-color": "^3.7.2",
    "@tiptap/extension-highlight": "^3.7.2",
    "@tiptap/extension-image": "^3.7.2",
    "@tiptap/extension-text-align": "^3.7.2",
    "@tiptap/extension-text-style": "^3.7.2",
    "@tiptap/react": "^3.7.2",
    "@tiptap/starter-kit": "^3.7.2",
    "class-variance-authority": "^0.7.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "input-otp": "^1.4.2",
    "linkify-react": "^4.3.2",
    "lucide-react": "^0.546.0",
    "motion": "^12.23.24",
    "next": "16.0.0",
    "nuqs": "^2.7.2",
    "react": "19.2.0",
    "react-aria-components": "^1.13.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.2.0",
    "react-dropzone": "^14.3.8",
    "sonner": "^2.0.7",
    "vaul": "^1.1.2"
  }
}
